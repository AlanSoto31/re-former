
<%= form_for @user, url: {action: "create"} do |f| %>
  User Name: <%= f.text_field :user_name, placeholder: "Only first name" %><br />
  Email: <%= f.email_field :email, placeholder: "example@hotmail.com" %><br />
  Password: <%= f.password_field :password, placeholder: "xxxxx" %><br />
  <%= f.submit "Create" %>
<% end %>
